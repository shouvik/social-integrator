{
  "name": "oauth-connector-sdk",
  "version": "1.0.0",
  "description": "Unified OAuth connector SDK for Google, GitHub, Reddit, Twitter, and RSS",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json",
    "./core/normalizer/schema.json": "./src/core/normalizer/schema.json"
  },
  "files": [
    "dist",
    "src/core/normalizer/schema.json",
    "README.md",
    "LICENSE",
    "package.json"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "coverage": "vitest run --coverage",
    "lint": "eslint src/**/*.ts",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write src/**/*.ts",
    "generate:schema": "tsx scripts/generate-schema.ts",
    "semantic-release": "semantic-release",
    "example:up": "docker compose -f examples/express-app/compose.yml up -d",
    "example:down": "docker compose -f examples/express-app/compose.yml down",
    "example:start": "cd examples/express-app && npm run dev",
    "example:auth-urls": "tsx examples/express-app/scripts/print-auth-urls.ts",
    "example": "npm run example:up && npm run example:auth-urls",
    "prepare": "husky"
  },
  "dependencies": {
    "@keyv/postgres": "^2.1.0",
    "@keyv/redis": "^3.0.0",
    "axios": "^1.6.2",
    "jose": "^5.1.3",
    "keyv": "^5.4.0",
    "openid-client": "^5.6.1",
    "p-queue": "^8.0.1",
    "prom-client": "^15.1.0",
    "redis": "^4.6.0",
    "rss-parser": "^3.13.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.66.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.207.0",
    "@opentelemetry/sdk-node": "^0.207.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.6",
    "@types/node": "^20.10.5",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@vitest/coverage-v8": "^1.0.4",
    "eslint": "^8.55.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "nock": "^13.4.0",
    "prettier": "^3.1.1",
    "semantic-release": "^24.2.9",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.0.4",
    "zod-to-json-schema": "^3.24.6"
  },
  "optionalDependencies": {
    "@octokit/core": "^5.0.2",
    "@octokit/plugin-retry": "^6.0.1",
    "twitter-api-v2": "^1.15.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "oauth",
    "oauth2",
    "authentication",
    "authorization",
    "sdk",
    "connector",
    "google",
    "github",
    "reddit",
    "twitter",
    "rss",
    "typescript",
    "pkce",
    "token-management"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": ""
  },
  "bugs": {
    "url": ""
  },
  "homepage": "",
  "packageManager": "yarn@1.22.19+sha1.4ba7fc5c6e704fce2066ecbfb0b0d8976fe62447",
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  }
}
