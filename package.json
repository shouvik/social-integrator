    "test:coverage": "vitest run --coverage",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts"
  },
  "dependencies": {
    "openid-client": "^5.6.1",
    "axios": "^1.6.2",
    "p-queue": "^8.0.1",
    "keyv": "^5.4.0",
    "@keyv/redis": "^3.0.0",
    "@keyv/postgres": "^2.1.0",
    "zod": "^3.22.4",
    "jose": "^5.1.3",
    "rss-parser": "^3.13.0",
    "uuid": "^9.0.1",
    "prom-client": "^15.1.0",
    "winston": "^3.11.0",
    "redis": "^4.6.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@types/uuid": "^9.0.7",
    "typescript": "^5.3.3",
    "vitest": "^1.0.4",
    "@vitest/coverage-v8": "^1.0.4",
    "nock": "^13.4.0",
    "tsx": "^4.7.0",
    "eslint": "^8.55.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "prettier": "^3.1.1"
